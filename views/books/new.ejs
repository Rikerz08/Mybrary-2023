<h2>New Book</h2>
<!-- allow form to have multiple parts cuz of the file part for the cover image -->
<form action="/books/new" method="POST" enctype="multipart/form-data">
  <%- include('./_form_fields.ejs')%>
  <!-- href is for transitioning into links -->
  <a href="/books">Cancel</a>
  <!-- button is for actually executing submission form -->
  <button type="submit">Create</button>
</form>
